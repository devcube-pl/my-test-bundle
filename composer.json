{
  "name": "devcube/my-test-bundle",
  "description": "Testowy bundle",
  "keywords": [
    "symfony",
    "szkolenie"
  ],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub ≈Åukasik",
      "email": "kuba@devcube.pl"
    }
  ],
  "require": {
    "php": "^7.1 || ^8.0",
    "symfony/framework-bundle": "^4.4 || ^5.2"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Devcube\\Bundle\\MyTestBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
